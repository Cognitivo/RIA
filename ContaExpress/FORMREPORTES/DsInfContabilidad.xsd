                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT CONVERT(datetime, CONVERT(varchar(10), VENTAS.FECHAVENTA, 103), 103) AS FECHAVENTA, VENTAS.NUMVENTA, 
                         CASE dbo.VENTAS.ESTADO WHEN 1 THEN dbo.CLIENTES.NOMBRE ELSE 'FACTURA ANULADA' END AS NOMBRE, CASE dbo.VENTAS.ESTADO WHEN 1 THEN dbo.CLIENTES.RUC ELSE '0' END AS RUC, 
                         CASE dbo.VENTAS.ESTADO WHEN 1 THEN (dbo.VENTAS.TOTALEXENTA) ELSE '0' END AS TOTALEXENTA, CASE dbo.VENTAS.ESTADO WHEN 1 THEN SUM(dbo.VENTAS.TOTALIVA) ELSE '0' END AS TOTALIVA, 
                         CASE dbo.VENTAS.ESTADO WHEN 1 THEN SUM(dbo.VENTAS.TOTAL5) ELSE '0' END AS TOTAL5, CASE dbo.VENTAS.ESTADO WHEN 1 THEN SUM(dbo.VENTAS.TOTAL10) ELSE '0' END AS TOTAL10, 
                         CASE dbo.VENTAS.ESTADO WHEN 1 THEN SUM(dbo.VENTAS.TOTALGRAVADO5) - SUM(dbo.VENTAS.TOTAL5) ELSE '0' END AS TOTALGRAVADO5, 
                         CASE dbo.VENTAS.ESTADO WHEN 1 THEN SUM(dbo.VENTAS.TOTALGRAVADO10) - SUM(dbo.VENTAS.TOTAL10) ELSE '0' END AS TOTALGRAVADO10, 
                         CASE dbo.VENTAS.ESTADO WHEN 1 THEN SUM(dbo.VENTAS.TOTALVENTA) ELSE '0' END AS TOTALVENTA, VENTAS.TIPOVENTA, SUCURSAL.DESSUCURSAL
