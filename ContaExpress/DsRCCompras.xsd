GROUP BY COMPRAS.NUMCOMPRA, PROVEEDOR.NOMBRE, PROVEEDOR.RUC_CIN, COMPRAS.MODALIDADPAGO, COMPRAS.FECHACOMPRA, COMPRAS.TOTALCOMPRA, COMPRAS.ESTADO, COMPRAS.CODSUCURSAL, 
