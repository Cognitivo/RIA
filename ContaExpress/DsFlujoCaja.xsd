                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        ISNULL(SUM(FACTURACOBRAR.IMPORTECUOTA), 0) AS SALIDA, ISNULL(SUM(FACTURAPAGAR.IMPORTECUOTA), 0) AS ENTRADA, ISNULL(CONVERT(DATETIME, CONVERT(varchar(10), 
