FROM            FACTURACOBRAR FULL OUTER JOIN
                         FACTURAPAGAR ON FACTURACOBRAR.FECHAVCTO = FACTURAPAGAR.FECHAVCTO
WHERE        (FACTURACOBRAR.FECHAVCTO &gt;= CONVERT(DATETIME, @FECHA1, 103)) AND (FACTURACOBRAR.FECHAVCTO &lt;= CONVERT(DATETIME, @FECHA2, 103)) OR
                         (FACTURAPAGAR.FECHAVCTO &gt;= CONVERT(DATETIME, @FECHA1, 103)) AND (FACTURAPAGAR.FECHAVCTO &lt;= CONVERT(DATETIME, @FECHA2, 103))
GROUP BY ISNULL(CONVERT(DATETIME, CONVERT(varchar(10), FACTURACOBRAR.FECHAVCTO, 103), 103), CONVERT(DATETIME, CONVERT(varchar(10), FACTURAPAGAR.FECHAVCTO, 103), 103))</CommandText>
