            <MainSource>
              <DbSource ConnectionRef="GESTIONConnectionString2 (MySettings)" DbObjectName="GuazuNuevo.dbo.CLIENTES" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT CLIENTES.CODCLIENTE, CLIENTES.NUMCLIENTE, CLIENTES.NOMBRE, SUM(VENTASDETALLE.CANTIDADVENTA) AS TOTALCANTIDAD, 
                         SUM(VENTASDETALLE.PRECIOVENTABRUTO) AS TOTALCONIVA, SUM(ISNULL(VENTASDETALLE.COSTOPROMEDIO, 0) * VENTASDETALLE.CANTIDADVENTA) 
                         AS TOTALCOSTO, SUM(VENTASDETALLE.IMPORTEGRABADODIEZ) / 11 AS IVA10, SUM(VENTASDETALLE.IMPORTEGRABADOCINCO) / 21 AS IVA5, 
                         RTRIM(CLIENTES.NOMBRE) + ' (' + ISNULL(CONVERT(VARCHAR, CLIENTES.NUMCLIENTE), '') + ')' AS NUMNOMCLIENTE, CLIENTES.NOMBREFANTASIA
FROM            VENTAS INNER JOIN
                         VENTASDETALLE ON VENTAS.CODVENTA = VENTASDETALLE.CODVENTA INNER JOIN
                         CLIENTES ON VENTAS.CODCLIENTE = CLIENTES.CODCLIENTE
WHERE        (VENTAS.FECHAVENTA &gt;= @desde) AND (VENTAS.FECHAVENTA &lt;= @hasta) AND (VENTAS.ESTADO = 1)
GROUP BY CLIENTES.NOMBRE, CLIENTES.NUMCLIENTE, CLIENTES.CODCLIENTE, CLIENTES.NOMBREFANTASIA
ORDER BY TOTALCONIVA DESC</CommandText>
