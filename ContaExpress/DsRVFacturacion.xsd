FROM            (SELECT        TOP (100) PERCENT MAX(CASE WHEN TIPO = 'VENTA_ANUL' OR
                                                    TIPO = 'NC_ANUL' THEN 0 ELSE TOTAL END) AS TOTALFACTURAS, SUM(CASE WHEN TIPO = 'VENTA_ANUL' OR
                                                    TIPO = 'NC_ANUL' THEN 0 ELSE SALDO END) AS SALDOFAC, SUM(ISNULL(IMPCOBRO / COTIZACION1, 0)) AS IMPORTECOBRORECIBOS, MAX(CODCLIENTE) AS CODCLIENTE
