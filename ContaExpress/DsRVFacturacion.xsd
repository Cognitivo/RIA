                         PAIS ON CIUDAD.CODPAIS = PAIS.CODPAIS
WHERE        (VENTAS.ESTADO = 0)
GROUP BY Z.DESZONA, CODIGOS.CODIGO, LTRIM(PRODUCTOS.DESPRODUCTO) + ' ' + LTRIM(ISNULL(CODIGOS.DESCODIGO1, '')) 
                         + ' ' + LTRIM(ISNULL(CODIGOS.DESCODIGO2, ''))
