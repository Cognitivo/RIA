                                                                              V.TIPOVENTA = 0 THEN 'VENTA_CONT' WHEN V.ESTADO = 1 AND V.TIPOVENTA = 1 THEN 'VENTA_CRED' WHEN V.ESTADO = 1 AND (V.TIPOVENTA &gt; 1) 
                                                                              THEN 'VENTA_NOPAG' WHEN V.ESTADO = 2 THEN 'VENTA_ANUL' END AS TIPO, V.CODVENTA AS CODCOMP, VFCORIG.IMPORTE AS IMPCOBRO, UPPER(VFCORIG.DESTIPOCOBRO) 
                                                                              AS DESTIPOCOBRO, VFCORIG.FECHAREGISTROCOB, VFCORIG.FECHACOBRO, VFCORIG.NRORECIBO, VFCORIG.NUMDEVOLUCION,
