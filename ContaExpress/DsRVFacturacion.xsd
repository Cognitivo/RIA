                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        SUM(TOTALFACTURAS) AS TOTALFACTURAS, SUM(IMPORTECOBRORECIBOS) AS IMPORTECOBRORECIBOS, SUM(TOTALFACTURAS) - SUM(IMPORTECOBRORECIBOS) AS SALDOFAC, MAX(CODCLIENTE) 
