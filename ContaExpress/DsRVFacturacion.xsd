1.RUC, DEV.MOTIVOANULADO, DEV.COTIZACION1 FROM DEVOLUCION AS DEV INNER JOIN CLIENTES AS CLIENTES_1 ON DEV.CODCLIENTE = CLIENTES_1.CODCLIENTE LEFT OUTER JOIN CIUDAD AS CIUDAD_1 ON CLIENTES_1.CODCIUDAD = CIUDAD_1.CODCIUDAD WHERE (DEV.TOTALDEVOLUCION - ISNULL ((SELECT SUM(IMPORTE) AS PAGADO FROM VENTASFORMACOBRO AS VENTASFORMACOBRO_2 WHERE (NUMDEVOLUCION = DEV.NUMDEVOLUCION)), 0) &lt;&gt; 0) AND (DEV.ESTADO &lt;&gt; 0)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="FillByPlan" GenerateMethods="Both" GenerateShortCommands="true" GeneratorGetMethodName="GetDataByPlan" GeneratorSourceName="FillByPlan" GetMethodModifier="Public" GetMethodName="GetDataByPlan" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetDataByPlan" UserSourceName="FillByPlan">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT TOTAL, CASE WHEN TIPO = 'VENTA_ANUL' OR
                         TIPO = 'NC_ANUL' THEN 0 ELSE SALDO END AS SALDO, NROCOMPROBANTE, FECHA, TIPO, IMPCOBRO, CASE WHEN DESTIPOCOBRO = 'Nota de Credito' OR
                         DESTIPOCOBRO = 'Nota de Crédito' THEN 'NOTA DE CREDITO' ELSE 'RECIBO' END AS DESTIPOCOBRO, FECHAREGISTROCOB, FECHACOBRO, CASE WHEN DESTIPOCOBRO = 'Nota de Credito' OR
                         DESTIPOCOBRO = 'Nota de Crédito' THEN NUMDEVOLUCION ELSE NRORECIBO END AS NRORECIBO, TOTALRECIBO, NUMCLIENTE, NOMBRE, DIRECCION, DESCIUDAD, TELEFONO, RUC, NOMBREFANTASIA, 
                         MOTIVOANULADO, COTIZACION1
FROM            (SELECT        TOP (100) PERCENT V.TOTALVENTA AS TOTAL,
                                                        (SELECT        SUM(SALDOCUOTA) AS Expr1
                                                          FROM            FACTURACOBRAR AS FAC
                                                          WHERE        (V.CODVENTA = CODVENTA)
                                                          GROUP BY CODVENTA) AS SALDO, V.NUMVENTA AS NROCOMPROBANTE, V.FECHAVENTA AS FECHA, CASE WHEN V.ESTADO = 1 AND 
                                                    V.TIPOVENTA = 0 THEN 'VENTA_CONT' WHEN V.ESTADO = 1 AND V.TIPOVENTA = 1 THEN 'VENTA_CRED' WHEN V.ESTADO = 1 AND (V.TIPOVENTA &gt; 1) 
                                                    THEN 'VENTA_NOPAG' WHEN V.ESTADO = 2 THEN 'VENTA_ANUL' END AS TIPO, V.CODVENTA AS CODCOMP, VFCORIG.IMPORTE AS IMPCOBRO, UPPER(VFCORIG.DESTIPOCOBRO) 
                                                    AS DESTIPOCOBRO, VFCORIG.FECHAREGISTROCOB, VFCORIG.FECHACOBRO, VFCORIG.NRORECIBO, VFCORIG.NUMDEVOLUCION,
                                                        (SELECT        SUM(IMPORTE) AS Expr1
                                                          FROM            VENTASFORMACOBRO
                                                          WHERE        (NRORECIBO = VFCORIG.NRORECIBO)
                                                          GROUP BY NRORECIBO) AS TOTALRECIBO, VFCORIG.CODCOBRO, CLIENTES.NUMCLIENTE, CLIENTES.NOMBRE, CLIENTES.DIRECCION, CIUDAD.DESCIUDAD, CLIENTES.TELEFONO, 
                                                    V.CODCLIENTE, CLIENTES.NOMBREFANTASIA, CLIENTES.RUC, V.MOTIVOANULADO, VFCORIG.COTIZACION1
                          FROM            VENTAS AS V INNER JOIN
                                                    CLIENTES ON V.CODCLIENTE = CLIENTES.CODCLIENTE LEFT OUTER JOIN
                                                    CIUDAD ON CLIENTES.CODCIUDAD = CIUDAD.CODCIUDAD LEFT OUTER JOIN
                                                    VENTASFORMACOBRO AS VFCORIG ON V.CODVENTA = VFCORIG.CODVENTA
                          WHERE        (V.ESTADO &lt;&gt; 0)
                          UNION ALL
                          SELECT        - DEV.TOTALDEVOLUCION AS TOTAL, - (DEV.TOTALDEVOLUCION - ISNULL
                                                       ((SELECT        SUM(IMPORTE) AS PAGADO
                                                           FROM            VENTASFORMACOBRO AS VENTASFORMACOBRO_3
                                                           WHERE        (NUMDEVOLUCION = DEV.NUMDEVOLUCION)), 0)) AS SALDO, DEV.NUMDEVOLUCION AS NROCOMPROBANTE, DEV.FECHADEVOLUCION AS FECHA, 
                                                   CASE DEV.ESTADO WHEN 1 THEN 'NC_VENTA' WHEN 2 THEN 'NC_ANUL' END AS TIPO, DEV.CODDEVOLUCION AS CODCOMP, NULL AS IMPCOBRO, NULL AS DESTIPOCOBRO, NULL 
                                                   AS FECHAREGISTROCOB, NULL AS FECHACOBRO, NULL AS NRORECIBO, DEV.NUMDEVOLUCION, 0 AS TOTALRECIBO, NULL AS CODCOBRO, CLIENTES_1.NUMCLIENTE, CLIENTES_1.NOMBRE, 
                                                   CLIENTES_1.DIRECCION, CIUDAD_1.DESCIUDAD, CLIENTES_1.TELEFONO, DEV.CODCLIENTE, CLIENTES_1.NOMBREFANTASIA, CLIENTES_1.RUC, DEV.MOTIVOANULADO, DEV.COTIZACION1
                          FROM            DEVOLUCION AS DEV INNER JOIN
                                                   CLIENTES AS CLIENTES_1 ON DEV.CODCLIENTE = CLIENTES_1.CODCLIENTE LEFT OUTER JOIN
                                                   CIUDAD AS CIUDAD_1 ON CLIENTES_1.CODCIUDAD = CIUDAD_1.CODCIUDAD
                          WHERE        (DEV.TOTALDEVOLUCION - ISNULL
                                                       ((SELECT        SUM(IMPORTE) AS PAGADO
                                                           FROM            VENTASFORMACOBRO AS VENTASFORMACOBRO_2
                                                           WHERE        (NUMDEVOLUCION = DEV.NUMDEVOLUCION)), 0) &lt;&gt; 0) AND (DEV.ESTADO &lt;&gt; 0)) AS MOVIMIENTOSCLIENTE
WHERE        (NUMCLIENTE IN
                             (SELECT        item
                               FROM            dbo.fnPartir(@Plan, ',') AS fnPartir_1)) OR
                         (LEN(ISNULL(@Plan, '')) = 0) AND (FECHA &gt;= @fechaini) AND (FECHA &lt;= @fechafin)</CommandText>
