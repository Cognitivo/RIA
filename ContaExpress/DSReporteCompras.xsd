                         SUCURSAL ON COMPRAS.CODSUCURSAL = SUCURSAL.CODSUCURSAL INNER JOIN
                         PROVEEDOR ON COMPRAS.CODPROVEEDOR = PROVEEDOR.CODPROVEEDOR INNER JOIN
                         COMPRASDETALLE ON COMPRAS.CODCOMPRA = COMPRASDETALLE.CODCOMPRA LEFT OUTER JOIN
