          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="RCIVACOMPRASTableAdapter" GeneratorDataComponentClassName="RCIVACOMPRASTableAdapter" Name="RCIVACOMPRAS" UserDataComponentName="RCIVACOMPRASTableAdapter">
