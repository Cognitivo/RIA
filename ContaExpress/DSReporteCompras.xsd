                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT COMPRAS.FECHACOMPRA AS FECHA, 
                         CASE dbo.COMPRAS.MODALIDADPAGO WHEN 0 THEN 'COMPRA_CONT' WHEN 1 THEN 'COMPRA_CRED' END AS TIPO, SUBSTRING(COMPRAS.NUMCOMPRA, 
                         LEN(COMPRAS.NUMCOMPRA) - 5, 6) AS NUMCOMPRA, PROVEEDOR.NUMPROVEEDOR, PROVEEDOR.NOMBRE, MONEDA.SIMBOLO, 
                         CASE IVA WHEN 10 THEN SUM(dbo.COMPRASDETALLE.IMPORTEGRAVADO10) - SUM(dbo.COMPRASDETALLE.IMPORTEGRAVADO10) 
                         / 11 WHEN 5 THEN SUM(dbo.COMPRASDETALLE.IMPORTEGRAVADO5) - SUM(dbo.COMPRASDETALLE.IMPORTEGRAVADO5) / 21 END AS NETO, 
                         COMPRASDETALLE.IVA, COMPRAS.CODPROVEEDOR, SUM(COMPRASDETALLE.IMPORTEGRAVADO10) / 11 AS IVA10, SUM(COMPRASDETALLE.IMPORTEGRAVADO5) 
