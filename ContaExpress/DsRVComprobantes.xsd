                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT SUM(VENTASDETALLE.CANTIDADVENTA) AS CANTIDAD, SUM(VENTASDETALLE.PRECIOVENTABRUTO) AS TOTAL, CODIGOS.CODIGO, 
                         LTRIM(PRODUCTOS.DESPRODUCTO) + ' ' + LTRIM(ISNULL(CODIGOS.DESCODIGO1, '')) + ' ' + LTRIM(ISNULL(CODIGOS.DESCODIGO2, '')) AS PRODUCTO, 
                         SUM(VENTASDETALLE.CANTIDADVENTA) * CODIGOS.PRECIO AS COSTO, SUM(VENTASDETALLE.PRECIOVENTANETO) AS TOTALSINIVA, FAMILIA.CODFAMILIA, 
                         FAMILIA.DESFAMILIA, CLIENTES.NOMBREFANTASIA, CLIENTES.CODCLIENTE, CLIENTES.NUMCLIENTE, CLIENTES.NOMBRE
FROM            VENTASDETALLE INNER JOIN
                         PRODUCTOS ON PRODUCTOS.CODPRODUCTO = VENTASDETALLE.CODPRODUCTO INNER JOIN
                         CODIGOS ON PRODUCTOS.CODPRODUCTO = CODIGOS.CODPRODUCTO INNER JOIN
                         VENTAS ON VENTASDETALLE.CODVENTA = VENTAS.CODVENTA INNER JOIN
                         CLIENTES ON CLIENTES.CODCLIENTE = VENTAS.CODCLIENTE LEFT OUTER JOIN
                         FAMILIA ON PRODUCTOS.CODFAMILIA = FAMILIA.CODFAMILIA
WHERE        (VENTAS.ESTADO = 1)
GROUP BY CODIGOS.CODIGO, LTRIM(PRODUCTOS.DESPRODUCTO) + ' ' + LTRIM(ISNULL(CODIGOS.DESCODIGO1, '')) + ' ' + LTRIM(ISNULL(CODIGOS.DESCODIGO2, '')), 
                         CODIGOS.PRECIO, FAMILIA.CODFAMILIA, FAMILIA.DESFAMILIA, CLIENTES.NOMBREFANTASIA, CLIENTES.CODCLIENTE, CLIENTES.NUMCLIENTE, 
                         CLIENTES.NOMBRE
ORDER BY PRODUCTO</CommandText>
