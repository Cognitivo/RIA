                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        CODIGOS.CODIGO, PRODUCTOS.DESPRODUCTO + ' ' + RTRIM(ISNULL(CODIGOS.DESCODIGO1, '')) + ' ' + RTRIM(ISNULL(CODIGOS.DESCODIGO2, '')) 
