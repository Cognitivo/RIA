rSourceName="FillOByNumProv">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT TOTAL, SALDO, NROCOMPROBANTE, FECHA, TIPO, NUMPROVEEDOR, NOMBRE, CODPROVEEDOR, DIRECCION, TELEFONO, 
                         DESCIUDAD, CODCOMPRA
FROM            (SELECT        C.TOTALCOMPRA AS TOTAL,
                                                        (SELECT        SUM(SALDOCUOTA) AS Expr1
                                                          FROM            dbo.FACTURAPAGAR
                                                          WHERE        (C.CODCOMPRA = CODCOMPRA)) AS SALDO, C.NUMCOMPRA AS NROCOMPROBANTE, C.FECHACOMPRA AS FECHA, 
                                                    CASE WHEN C.MODALIDADPAGO = 0 THEN 'CONTADO' WHEN C.MODALIDADPAGO = 1 THEN 'CREDITO' ELSE 'NO PAGAR' END AS TIPO, 
                                                    dbo.PROVEEDOR.NUMPROVEEDOR, dbo.PROVEEDOR.NOMBRE, dbo.PROVEEDOR.CODPROVEEDOR, dbo.PROVEEDOR.DIRECCION, 
                                                    CIUDAD.DESCIUDAD, dbo.PROVEEDOR.TELEFONO, C.CODCOMPRA
                          FROM            dbo.COMPRAS AS C INNER JOIN
                                                    dbo.PROVEEDOR ON C.CODPROVEEDOR = dbo.PROVEEDOR.CODPROVEEDOR LEFT OUTER JOIN
                                                    dbo.ZONA ON dbo.PROVEEDOR.CODZONA = dbo.ZONA.CODZONA LEFT OUTER JOIN
                                                    dbo.CIUDAD AS CIUDAD ON dbo.ZONA.CODCIUDAD = CIUDAD.CODCIUDAD
                          WHERE        (C.ESTADO = 1) AND
                                                        ((SELECT        SUM(SALDOCUOTA) AS Expr1
                                                            FROM            dbo.FACTURAPAGAR AS FACTURAPAGAR_1
                                                            WHERE        (C.CODCOMPRA = CODCOMPRA)) &gt; 0)
                          UNION ALL 
                          SELECT        - DEV.TOTALDEVOLUCION AS TOTAL, -
                                                       (SELECT        SALDO AS PAGADO
                                                         FROM            dbo.DEVOLUCIONCOMPRA AS DEVOLUCIONCOMPRA_2
                                                         WHERE        (CODDEVOLUCION = DEV.CODDEVOLUCION)) AS SALDO, DEV.NUMDEVOLUCION AS NROCOMPROBANTE, 
                                                   DEV.FECHADEVOLUCION AS FECHA, 'NC_COMPRA' AS TIPO, PROVEEDOR_1.NUMPROVEEDOR, PROVEEDOR_1.NOMBRE, 
                                                   DEV.CODPROVEEDOR, PROVEEDOR_1.DIRECCION, CIUDAD_1.DESCIUDAD, PROVEEDOR_1.TELEFONO, 
                                                   DEV.CODDEVOLUCION AS CODCOMP
                          FROM            dbo.DEVOLUCIONCOMPRA AS DEV INNER JOIN
                                                   dbo.PROVEEDOR AS PROVEEDOR_1 ON DEV.CODPROVEEDOR = PROVEEDOR_1.CODPROVEEDOR LEFT OUTER JOIN
                                                   dbo.ZONA AS ZONA_1 ON PROVEEDOR_1.CODZONA = ZONA_1.CODZONA LEFT OUTER JOIN
                                                   dbo.CIUDAD AS CIUDAD_1 ON ZONA_1.CODCIUDAD = CIUDAD_1.CODCIUDAD
                          WHERE        (DEV.ESTADO = 1) AND
                                                       ((SELECT        SALDO AS PAGADO
                                                           FROM            dbo.DEVOLUCIONCOMPRA AS DEVOLUCIONCOMPRA_2
                                                           WHERE        (CODDEVOLUCION = DEV.CODDEVOLUCION)) &lt;&gt; 0)
                          UNION
                          SELECT        dbo.COMPRAS.TOTALCOMPRA AS TOTAL,
                                                       (SELECT        SUM(IMPORTE) AS Expr1
                                                         FROM            dbo.COMPRASFORMAPAGO AS CFPAUX
                                                         WHERE        (CODCOMPRA = CFP.CODCOMPRA) AND (FECHAREGISTROPAG &gt;= @fhasta)) AS SALDO, dbo.COMPRAS.NUMCOMPRA, 
                                                   dbo.COMPRAS.FECHACOMPRA, CASE dbo.COMPRAS.MODALIDADPAGO WHEN 0 THEN 'CONTADO' ELSE 'CREDITO' END AS TIPO, 
