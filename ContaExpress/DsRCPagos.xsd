                         dbo.MONEDA ON dbo.COMPRASFORMAPAGO.CODMONEDA = dbo.MONEDA.CODMONEDA LEFT OUTER JOIN
                         dbo.COMPRAS ON dbo.COMPRASFORMAPAGO.CODCOMPRA = dbo.COMPRAS.CODCOMPRA INNER JOIN
                         dbo.PROVEEDOR ON dbo.COMPRAS.CODPROVEEDOR = dbo.PROVEEDOR.CODPROVEEDOR LEFT OUTER JOIN
                         dbo.BANCO ON dbo.COMPRASFORMAPAGO.CH_TA_TR_CODBANCO = dbo.BANCO.CODBANCO LEFT OUTER JOIN
                         dbo.aperturas ON dbo.COMPRASFORMAPAGO.id_apertura = dbo.aperturas.id_apertura LEFT OUTER JOIN
                         dbo.CAJA ON dbo.aperturas.id_caja = dbo.CAJA.NUMCAJA
GROUP BY dbo.MONEDA.SIMBOLO, dbo.PROVEEDOR.NUMPROVEEDOR, dbo.PROVEEDOR.CODPROVEEDOR, dbo.COMPRASFORMAPAGO.NRORECIBO, 
