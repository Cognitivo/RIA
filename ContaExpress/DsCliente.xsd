                         ISNULL(CLIENTES.CLIENTEEXENTO, 0) AS CLIENTEEXENTO, CLIENTES.CODIGOCOMISION, CLIENTES.NUMEROTOL, CLIENTES.FECHANACIMIENTO, CLIENTES.AUTORIZADO, CLIENTES.FECAUTORIZACION, 
                         CLIENTES.CODCATEGORIACLIENTE, CLIENTES.CODCLIENTEPRINCIPAL, CLIENTES.IMAGENCLIENTE, CLIENTES.CODPAIS, CLIENTES.TIMBRADOFACTURA, CLIENTES.TIMBRADORETENCION, CLIENTES.SEXO, 
                         CLIENTES.PROVEEDOR_ID, ISNULL(CLIENTES.PERSONAJURIDICA, 0) AS PERSONAJURIDICA, CLIENTES.TELEFONO1, CLIENTES.EMAIL1, CLIENTES.CODCIUDAD, CLIENTES.NUMPROVEEDORPARACLIENTE, 
                         CLIENTES.TIPOVENTA, CLIENTES.MATRIZ, CLIENTES.DV, CLIENTES.CODDEPARTAMENTO, CLIENTES.CUSTOMFIELD, CLIENTES.TIPORELACION, CLIENTES.RELACION, CLIENTES.NOMBREFANTASIA, 
                         CLIENTES.NOMBRECONTACTO, CLIENTES.NUMCLIENTE, CLIENTES.CANTCUOTAS, CLIENTES.CODEMPRESA, CLIENTES.CODUSUARIO, CLIENTES.CODVENDEDOR, CLIENTES.CODSUCURSAL, 
                         CLIENTES.EMPPRESACLIENTE, CLIENTES_1.NOMBRE AS CABECERA, CLIENTES.CODCLIENTE, CLIENTES.LINKMAPS, CLIENTES.REPRESENTANTELEGAL, CLIENTES.CODPOSTAL, 
                         CLIENTES.CONTACTOCORREO1, CLIENTES.CONTACTOCORREO2, CLIENTES.CONTACTOTELEFONO, CLIENTES.CONTACTOCELULAR
FROM            CLIENTES LEFT OUTER JOIN
                         CLIENTES AS CLIENTES_1 ON CLIENTES.RELACION = CLIENTES_1.CODCLIENTE
ORDER BY CLIENTES.NOMBRE</CommandText>
