                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        CLIENTES.CODZONA, CLIENTES.CODCOBRADOR, CLIENTES.CODTIPOCLIENTE, CLIENTES.NOMBRE, CLIENTES.APELLIDO, ISNULL(RTRIM(CLIENTES.NOMBRE), '') + ' ' + ISNULL(LTRIM(CLIENTES.APELLIDO), '') 
                         AS CLIENTE, CLIENTES.NUMCEDULA, CLIENTES.RUC, CLIENTES.DIRECCION, CLIENTES.TELEFONO, CLIENTES.CELULAR, CLIENTES.FAX, CLIENTES.EMAIL, CLIENTES.WEB, CLIENTES.LIMCREDITO, 
