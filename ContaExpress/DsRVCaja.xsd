                    <CommandText>SELECT     id_caja, id_apertura, DESUSUARIO, fecha_apertura, fecha_cierre, SUM(TOTALEXENTA) AS TotalExenta, SUM(TOTALGRAVADO5) AS TotalGrav5, 
                      SUM(TOTALGRAVADO10) AS TotalGrav10, SUM(TOTALVENTA) AS TotalVenta, SUM(TOTALIVA) AS TotalIVA, id_moneda, DESMONEDA, DESSUCURSAL, fecha_mto, 
                      VALORFISCAL, DESCOMPROBANTE, NUMEROCAJA
FROM         (SELECT DISTINCT 
                                              dbo.aperturas.id_caja, dbo.USUARIO.DESUSUARIO, dbo.cierres.fecha_apertura, dbo.cierres.fecha_cierre, dbo.VENTAS.TOTALEXENTA, 
