                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT movimientos.id_movimiento, movimientos.id_apertura, movimientos.fecha_mto, movimientos.cotizacion1, movimientos.concepto,
                             (SELECT        movimientos.monto / movimientos.cotizacion1 AS Total
                               WHERE        (movimientos.entradasalida = 'entrada')) AS Entrada,
                             (SELECT        CASE WHEN id_compra IS NOT NULL 
                                                         THEN - movimientos.monto / movimientos.cotizacion1 ELSE movimientos.monto / movimientos.cotizacion1 END AS Total
                               WHERE        (movimientos.entradasalida = 'salida')) AS Salida, MONEDA.DESMONEDA, aperturas.fechapaertura, aperturas.apertura_num, USUARIO.DESUSUARIO, 
                         CAJA.NUMEROCAJA AS NUMCAJA, cierres.fecha_cierre, SUCURSAL.CODSUCURSAL, SUCURSAL.DESSUCURSAL, COMPRAS.NUMCOMPRA, VENTAS.NUMVENTA, 
                         movimientos.id_tipo_dinero, USUARIO.CODUSUARIO, VENTAS.FECHAVENTA
FROM            VENTASFORMACOBRO INNER JOIN
                         VENTAS ON VENTASFORMACOBRO.CODVENTA = VENTAS.CODVENTA FULL OUTER JOIN
                         aperturas LEFT OUTER JOIN
                         USUARIO ON aperturas.id_usuario = USUARIO.CODUSUARIO LEFT OUTER JOIN
                         CAJA ON aperturas.id_caja = CAJA.NUMCAJA FULL OUTER JOIN
                         movimientos LEFT OUTER JOIN
                         MONEDA ON movimientos.id_moneda = MONEDA.CODMONEDA ON aperturas.id_apertura = movimientos.id_apertura ON 
                         VENTASFORMACOBRO.CODVENTA = movimientos.id_venta AND VENTASFORMACOBRO.CODCOBRO = movimientos.id_cobro FULL OUTER JOIN
                         SUCURSAL ON movimientos.codsucursal = SUCURSAL.CODSUCURSAL FULL OUTER JOIN
                         COMPRASFORMAPAGO LEFT OUTER JOIN
                         COMPRAS ON COMPRASFORMAPAGO.CODCOMPRA = COMPRAS.CODCOMPRA ON movimientos.id_compra = COMPRASFORMAPAGO.CODCOMPRA AND 
                         movimientos.id_pago = COMPRASFORMAPAGO.CODPAGO FULL OUTER JOIN
                         cierres ON aperturas.id_apertura = cierres.id_apertura
WHERE        (movimientos.fecha_mto &gt;= @FechaDesde) AND (movimientos.fecha_mto &lt;= @FechaHasta) AND (CAJA.NUMEROCAJA LIKE @Caja) AND 
                         (SUCURSAL.CODSUCURSAL IN
                             (SELECT        item
                               FROM            dbo.fnPartir(@SUC, ',') AS fnPartir_1)) AND (MONEDA.DESMONEDA LIKE @moneda) AND (VENTAS.ESTADO = 1) AND 
                         (movimientos.id_tipo_dinero &lt;&gt; 5) OR
                         (movimientos.fecha_mto &gt;= @FechaDesde) AND (movimientos.fecha_mto &lt;= @FechaHasta) AND (CAJA.NUMEROCAJA LIKE @Caja) AND 
                         (SUCURSAL.CODSUCURSAL IN
                             (SELECT        item
                               FROM            dbo.fnPartir(@SUC, ',') AS fnPartir_1)) AND (MONEDA.DESMONEDA LIKE @moneda) AND (movimientos.id_tipo_dinero &lt;&gt; 5) AND 
                         (COMPRAS.ESTADO = 1) OR
                         (movimientos.fecha_mto &gt;= @FechaDesde) AND (movimientos.fecha_mto &lt;= @FechaHasta) AND (CAJA.NUMEROCAJA LIKE @Caja) AND 
                         (SUCURSAL.CODSUCURSAL IN
                             (SELECT        item
                               FROM            dbo.fnPartir(@SUC, ',') AS fnPartir_1)) AND (MONEDA.DESMONEDA LIKE @moneda) AND (VENTAS.ESTADO IS NULL) AND 
                         (movimientos.id_tipo_dinero &lt;&gt; 5) AND (COMPRAS.ESTADO IS NULL)
ORDER BY movimientos.fecha_mto</CommandText>
