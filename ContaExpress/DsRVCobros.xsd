                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        dbo.MONEDA.SIMBOLO, SUM(dbo.VENTASFORMACOBRO.IMPORTE) AS IMPORTE, dbo.CLIENTES.NUMCLIENTE, dbo.CLIENTES.NOMBRE, 
