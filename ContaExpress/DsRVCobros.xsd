                    <CommandText>SELECT        TOP (100) PERCENT NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, CODCLIENTE, NOMBRECLIENTE, TIPO, NUMVENTA, IMPORTECUOTA, 
                         CASE TIPOCOMP WHEN 0 THEN - SALDOCUOTA WHEN 1 THEN SALDOCUOTA END AS SALDOCUOTA, ATRASO, FECHAVENTA, CODTIPOCLIENTE, 
                         CODCATEGORIACLIENTE
FROM            (SELECT        TOP (100) PERCENT NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, CODCLIENTE, NOMBRECLIENTE, TIPO, NUMVENTA, IMPORTECUOTA, 
                                                    SALDOCUOTA, ATRASO, FECHAVENTA, CODTIPOCLIENTE, CODCATEGORIACLIENTE, 1 AS TIPOCOMP
                          FROM            (SELECT        NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, NOMBRECLIENTE, TIPO, NUMVENTA, IMPORTECUOTA, SALDOCUOTA, ATRASO, 
                                                                              CODTIPOCLIENTE, CODCATEGORIACLIENTE, CODCLIENTE, FECHAVENTA
                                                    FROM            (SELECT        FACTURACOBRAR.CODNUMEROCUOTA AS NUMEROCUOTA, FACTURACOBRAR.FECHAVCTO, CLIENTES.NUMCLIENTE, 
                                                                                                        VENTAS.FECHAVENTA, VENTAS.NOMBRECLIENTE, VENTAS.CODCLIENTE, 
                                                                                                        CASE MODALIDADPAGO WHEN '0' THEN 'VTA_CONT' WHEN '1' THEN 'VTA_CRED' WHEN '2' THEN 'VTA_BONIF' WHEN '3'
                                                                                                         THEN 'VTA_CAMBIO' ELSE 'OTROS' END AS TIPO, VENTAS.NUMVENTA, FACTURACOBRAR.IMPORTECUOTA, 
                                                                                                        FACTURACOBRAR.SALDOCUOTA, DATEDIFF(D, FACTURACOBRAR.FECHAVCTO, GETDATE()) AS ATRASO, 
                                                                                                        CLIENTES.CODTIPOCLIENTE, CLIENTES.CODCATEGORIACLIENTE
                                                                              FROM            FACTURACOBRAR INNER JOIN
                                                                                                        VENTAS ON FACTURACOBRAR.CODVENTA = VENTAS.CODVENTA INNER JOIN
                                                                                                        CLIENTES ON VENTAS.CODCLIENTE = CLIENTES.CODCLIENTE
                                                                              WHERE        (VENTAS.ESTADO = 1)) AS CLIENTESATRASADOSAUX) AS CLIENTESATRASADOS
                          WHERE        (SALDOCUOTA &gt; 0) AND (FECHAVENTA &lt;= CONVERT(DATETIME, @FECHAAL, 103))
                          UNION
                          SELECT        TOP (100) PERCENT NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, CODCLIENTE, NOMBRE, TIPODEVOLUCION, NUMDEVOLUCION, 
                                                   TOTALDEVOLUCION, SALDO, ATRASO, FECHADEVOLUCION, CODTIPOCLIENTE, CODCATEGORIACLIENTE, 0 AS TIPOCOMP
                          FROM            (SELECT        NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, NOMBRE, TIPODEVOLUCION, NUMDEVOLUCION, TOTALDEVOLUCION, SALDO, 
