                                                    FROM            (SELECT        1 AS NUMEROCUOTA, dbo.DEVOLUCION.FECHADEVOLUCION AS FECHAVCTO, CLIENTES_1.NUMCLIENTE, 
                                                                                                        dbo.DEVOLUCION.FECHADEVOLUCION, CLIENTES_1.NOMBRE, dbo.DEVOLUCION.CODCLIENTE, 
                                                                                                        CASE dbo.DEVOLUCION.TIPODEVOLUCION WHEN 'Por Cambio' THEN 'NC_CAMBIO' WHEN 'Bonificación' THEN 'NC_BONIF'
                                                                                                         ELSE 'NC_DEVOL' END AS TIPODEVOLUCION, dbo.DEVOLUCION.NUMDEVOLUCION, 
                                                                                                        dbo.DEVOLUCION.TOTALDEVOLUCION, dbo.DEVOLUCION.SALDO, DATEDIFF(D, dbo.DEVOLUCION.FECHADEVOLUCION, 
                                                                                                        GETDATE()) AS ATRASO, CLIENTES_1.CODTIPOCLIENTE, CLIENTES_1.CODCATEGORIACLIENTE
                                                                              FROM            dbo.DEVOLUCION INNER JOIN
                                                                                                        dbo.CLIENTES AS CLIENTES_1 ON dbo.DEVOLUCION.CODCLIENTE = CLIENTES_1.CODCLIENTE
                                                                              WHERE        (dbo.DEVOLUCION.ESTADO = 1)) AS CLIENTESATRASADOSAUX_1) AS CLIENTESATRASADOS_1
                          WHERE       (SALDO &gt; 0) AND (FECHADEVOLUCION &lt;= CONVERT(DATETIME, @FECHAAL, 103))) AS ATRASOS
WHERE        (CODTIPOCLIENTE IN
                             (SELECT        item
                               FROM            dbo.fnPartir(@LISTAP, ',') AS fnPartir_1)) AND (CODCATEGORIACLIENTE IN
                             (SELECT        item
                               FROM            dbo.fnPartir(@CATCLI, ',') AS fnPartir_1))
ORDER BY FECHAVCTO</CommandText>
