                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT TOTAL, SALDO, NROCOMPROBANTE, FECHA, TIPO, NUMCLIENTE, NOMBRE, CODCLIENTE, DIRECCION, DESCIUDAD, 
                         NOMBREFANTASIA, TELEFONO, CODCOMP
FROM            (SELECT        DEV.TOTALDEVOLUCION AS TOTAL, 
                                                       (SELECT        SALDO AS PAGADO
                                                         FROM            dbo.DEVOLUCION AS DEVOLUCION_2
                                                         WHERE        (CODDEVOLUCION = DEV.CODDEVOLUCION)) AS SALDO, DEV.NUMDEVOLUCION AS NROCOMPROBANTE, 
                                                   DEV.FECHADEVOLUCION AS FECHA, 
                                                   CASE DEV.TIPODEVOLUCION WHEN 'Por Cambio' THEN 'NC_CAMBIO' WHEN 'Bonificación' THEN 'NC_BONIF' ELSE 'NC_DEVOL' END AS TIPO, 
                                                   CLIENTES_1.NUMCLIENTE, CLIENTES_1.NOMBRE, DEV.CODCLIENTE, CLIENTES_1.DIRECCION, CIUDAD_1.DESCIUDAD, 
                                                   CLIENTES_1.TELEFONO, CLIENTES_1.NOMBREFANTASIA, DEV.CODDEVOLUCION AS CODCOMP
                          FROM            dbo.DEVOLUCION AS DEV INNER JOIN
                                                   dbo.CLIENTES AS CLIENTES_1 ON DEV.CODCLIENTE = CLIENTES_1.CODCLIENTE INNER JOIN
                                                   dbo.TIPOCLIENTE AS TIPOCLIENTE_1 ON CLIENTES_1.CODTIPOCLIENTE = TIPOCLIENTE_1.CODTIPOCLIENTE LEFT OUTER JOIN
                                                   dbo.CIUDAD AS CIUDAD_1 ON CLIENTES_1.CODCIUDAD = CIUDAD_1.CODCIUDAD
                          WHERE        (DEV.ESTADO = 1) AND (TIPOCLIENTE_1.CODTIPOCLIENTE IN
                                                       (SELECT        item
                                                         FROM            dbo.fnPartir(@LISTAP, ',') AS fnPartir_1)) AND
                                                       ((SELECT        SALDO AS PAGADO
                                                           FROM            dbo.DEVOLUCION AS DEVOLUCION_2
                                                           WHERE        (CODDEVOLUCION = DEV.CODDEVOLUCION)) &lt;&gt; 0) AND (CLIENTES_1.CODCATEGORIACLIENTE IN
                                                       (SELECT        item
                                                         FROM            dbo.fnPartir(@CATCLI, ',') AS fnPartir_1)) OR
                                                   (DEV.ESTADO = 1) AND (TIPOCLIENTE_1.CODTIPOCLIENTE IN
                                                       (SELECT        item
                                                         FROM            dbo.fnPartir(@LISTAP, ',') AS fnPartir_1)) AND (CLIENTES_1.CODCATEGORIACLIENTE IS NULL) AND
                                                       ((SELECT        SALDO AS PAGADO
                                                           FROM            dbo.DEVOLUCION AS DEVOLUCION_2
                                                           WHERE        (CODDEVOLUCION = DEV.CODDEVOLUCION)) &lt;&gt; 0)
                                               UNION ALL 
                          SELECT         dbo.DEVOLUCION.TOTALDEVOLUCION AS TOTAL, 
                                                       (SELECT        SUM(HABER) AS Expr1
                                                         FROM            dbo.CLIENTESCUENTACORRIENTE AS CLCTACTE3
                                                         WHERE        (IDTRANSACCION = 2) AND (CODDEVOLUCION = CCC.CODDEVOLUCION) AND CODVENTA IS NOT NULL AND (FECHACLIENTEMOV &gt; @fhasta)) AS SALDO, 
                                                   dbo.DEVOLUCION.NUMDEVOLUCION AS NROCOMPROBANTE, dbo.DEVOLUCION.FECHADEVOLUCION AS FECHA, 
                                                   CASE DEVOLUCION.TIPODEVOLUCION WHEN 'Por Cambio' THEN 'NC_CAMBIO' WHEN 'Bonificación' THEN 'NC_BONIF' ELSE 'NC_DEVOL' END AS
                                                    TIPO, CLIENTES_1.NUMCLIENTE, CLIENTES_1.NOMBRE, dbo.DEVOLUCION.CODCLIENTE, CLIENTES_1.DIRECCION, CIUDAD.DESCIUDAD, 
                                                   CLIENTES_1.TELEFONO, CLIENTES_1.NOMBREFANTASIA, dbo.DEVOLUCION.CODDEVOLUCION AS CODCOMP
                          FROM            dbo.CLIENTESCUENTACORRIENTE AS CCC INNER JOIN
                                                   dbo.DEVOLUCION ON CCC.CODDEVOLUCION = dbo.DEVOLUCION.CODDEVOLUCION INNER JOIN
                                                   dbo.CLIENTES AS CLIENTES_1 ON dbo.DEVOLUCION.CODCLIENTE = CLIENTES_1.CODCLIENTE LEFT OUTER JOIN
                                                   dbo.TIPOCLIENTE AS TIPOCLIENTE ON CLIENTES_1.CODTIPOCLIENTE = TIPOCLIENTE.CODTIPOCLIENTE LEFT OUTER JOIN
                                                   dbo.CIUDAD AS CIUDAD ON CLIENTES_1.CODCIUDAD = CIUDAD.CODCIUDAD
                          WHERE        (dbo.DEVOLUCION.ESTADO = 1) AND (CLIENTES_1.CODCATEGORIACLIENTE IN
                                                       (SELECT        item
                                                         FROM            dbo.fnPartir(@CATCLI, ',') AS fnPartir_1)) AND (TIPOCLIENTE.CODTIPOCLIENTE IN
                                                       (SELECT        item
                                                         FROM            dbo.fnPartir(@LISTAP, ',') AS fnPartir_1)) AND (CCC.FECHACLIENTEMOV &gt; @fhasta) OR
                                                   (dbo.DEVOLUCION.ESTADO = 1) AND (CLIENTES_1.CODCATEGORIACLIENTE IS NULL) AND (TIPOCLIENTE.CODTIPOCLIENTE IN
                                                       (SELECT        item
                                                         FROM            dbo.fnPartir(@LISTAP, ',') AS fnPartir_1)) AND (CCC.FECHACLIENTEMOV &gt; @fhasta)) AS COMPPENDIENTES
WHERE        (FECHA &gt;= @fdesde) AND (FECHA &lt;= @fhasta) AND (NUMCLIENTE IN
                             (SELECT        item
                               FROM            dbo.fnPartir(@Plan, ',') AS fnPartir_1))
ORDER BY NOMBRE, FECHA</CommandText>
