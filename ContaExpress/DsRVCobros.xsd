          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="RVANALISISDESALDOTableAdapter" GeneratorDataComponentClassName="RVANALISISDESALDOTableAdapter" Name="RVANALISISDESALDO" UserDataComponentName="RVANALISISDESALDOTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="GESTIONConnectionString2 (MySettings)" DbObjectName="SELECT TOP (100) PERCENT NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, CODCLIENTE, NOMBRECLIENTE, TIPO, NUMVENTA, IMPORTECUOTA, SALDOCUOTA, ATRASO, 0 AS COTIZACION, FECHAVENTA, CODTIPOCLIENTE, CODCATEGORIACLIENTE, 1 AS TIPOCOMP FROM (SELECT NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, NOMBRECLIENTE, TIPO, NUMVENTA, IMPORTECUOTA, SALDOCUOTA, ATRASO, CODTIPOCLIENTE, CODCATEGORIACLIENTE, CODCLIENTE, FECHAVENTA FROM (SELECT FACTURACOBRAR.CODNUMEROCUOTA AS NUMEROCUOTA, FACTURACOBRAR.FECHAVCTO, CLIENTES.NUMCLIENTE, VENTAS.FECHAVENTA, VENTAS.NOMBRECLIENTE, VENTAS.CODCLIENTE, CASE MODALIDADPAGO WHEN '0' THEN 'VTA_CONT' WHEN '1' THEN 'VTA_CRED' WHEN '2' THEN 'VTA_BONIF' WHEN '3' THEN 'VTA_CAMBIO' ELSE 'OTROS' END AS TIPO, VENTAS.NUMVENTA, FACTURACOBRAR.IMPORTECUOTA, FACTURACOBRAR.SALDOCUOTA, DATEDIFF(D, FACTURACOBRAR.FECHAVCTO, GETDATE()) AS ATRASO, CLIENTES.CODTIPOCLIENTE, CLIENTES.CODCATEGORIACLIENTE FROM FACTURACOBRAR INNER JOIN VENTAS ON FACTURACOBRAR.CODVENTA = VENTAS.CODVENTA INNER JOIN CLIENTES ON VENTAS.CODCLIENTE = CLIENTES.CODCLIENTE WHERE (VENTAS.ESTADO = 1)) AS CLIENTESATRASADOSAUX) AS CLIENTESATRASADOS WHERE (SALDOCUOTA &gt; 0) AND (FECHAVENTA &lt;= CONVERT(DATETIME, @FECHAAL, 103)) UNION SELECT TOP (100) PERCENT NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, CODCLIENTE, NOMBRE, TIPODEVOLUCION, NUMDEVOLUCION, TOTALDEVOLUCION, SALDO, ATRASO,COTIZACION, FECHADEVOLUCION, CODTIPOCLIENTE, CODCATEGORIACLIENTE, 0 AS TIPOCOMP FROM (SELECT NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, NOMBRE, TIPODEVOLUCION, NUMDEVOLUCION, TOTALDEVOLUCION, SALDO, ATRASO, COTIZACION, CODTIPOCLIENTE, CODCATEGORIACLIENTE, CODCLIENTE, FECHADEVOLUCION FROM (SELECT 1 AS NUMEROCUOTA, DEVOLUCION.FECHADEVOLUCION AS FECHAVCTO, CLIENTES_1.NUMCLIENTE, DEVOLUCION.FECHADEVOLUCION, CLIENTES_1.NOMBRE, DEVOLUCION.CODCLIENTE, CASE dbo.DEVOLUCION.TIPODEVOLUCION WHEN 'Por Cambio' THEN 'NC_CAMBIO' WHEN 'Bonificación' THEN 'NC_BONIF' ELSE 'NC_DEVOL' END AS TIPODEVOLUCION, DEVOLUCION.NUMDEVOLUCION, DEVOLUCION.TOTALDEVOLUCION, DEVOLUCION.SALDO, DATEDIFF(D, DEVOLUCION.FECHADEVOLUCION, GETDATE()) AS ATRASO, DEVOLUCION.COTIZACION1 AS COTIZACION, CLIENTES_1.CODTIPOCLIENTE, CLIENTES_1.CODCATEGORIACLIENTE FROM DEVOLUCION INNER JOIN CLIENTES AS CLIENTES_1 ON DEVOLUCION.CODCLIENTE = CLIENTES_1.CODCLIENTE WHERE (DEVOLUCION.ESTADO = 1)) AS VENCIMIENTOSAUX_1) AS VENCIMIENTOS_1 WHERE (SALDO &gt; 0) AND (FECHADEVOLUCION &lt;= CONVERT(DATETIME, @FECHAAL, 103))" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="FillByCliente" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetDataByCliente" GeneratorSourceName="FillByCliente" GetMethodModifier="Public" GetMethodName="GetDataByCliente" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetDataByCliente" UserSourceName="FillByCliente">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, CODCLIENTE, NOMBRECLIENTE, TIPO, NUMVENTA, 
				CASE WHEN TIPO = 'NC_DEVOL' OR TIPO = 'NC_BONIF' OR TIPO = 'NC_CAMBIO' THEN IMPORTECUOTA/COTIZACION ELSE IMPORTECUOTA END AS IMPORTECUOTA, 
                         CASE WHEN (TIPOCOMP = 0) AND (TIPO = 'NC_DEVOL' OR TIPO = 'NC_BONIF' OR TIPO = 'NC_CAMBIO') THEN -(SALDOCUOTA/COTIZACION) 
						 WHEN (TIPOCOMP = 1) AND (TIPO = 'NC_DEVOL' OR TIPO = 'NC_BONIF' OR TIPO = 'NC_CAMBIO') THEN SALDOCUOTA/COTIZACION  
						 WHEN (TIPOCOMP = 0) THEN -SALDOCUOTA
						 WHEN (TIPOCOMP = 1) THEN SALDOCUOTA END AS SALDOCUOTA, ATRASO, FECHAVENTA, CODTIPOCLIENTE, CODCATEGORIACLIENTE
FROM            (SELECT        TOP (100) PERCENT NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, CODCLIENTE, NOMBRECLIENTE, TIPO, NUMVENTA, IMPORTECUOTA, SALDOCUOTA, ATRASO, 0 AS COTIZACION, FECHAVENTA, CODTIPOCLIENTE, 
