                         CASE MODALIDADPAGO WHEN 0 THEN 'CONTADO' WHEN 1 THEN 'CREDITO' WHEN 2 THEN 'BONIFICACIÓN' WHEN 3 THEN 'CAMBIO' ELSE 'OTROS' END AS TIPO, dbo.VENTASFORMACOBRO.CABCOBRO, 
