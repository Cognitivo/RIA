          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="RVCLIENTESATRASADOSTableAdapter" GeneratorDataComponentClassName="RVCLIENTESATRASADOSTableAdapter" Name="RVCLIENTESATRASADOS" UserDataComponentName="RVCLIENTESATRASADOSTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="GESTIONConnectionString2 (MySettings)" DbObjectName="SELECT NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, NOMBRECLIENTE, TIPO, NUMVENTA, IMPORTECUOTA, SALDOCUOTA, CASE SALDOCUOTA WHEN 0 THEN 0 ELSE ATRASO END AS ATRASO, CODTIPOCLIENTE, CODCATEGORIACLIENTE, CODCLIENTE,FECHAVENTA FROM (SELECT dbo.FACTURACOBRAR.CODNUMEROCUOTA AS NUMEROCUOTA, dbo.FACTURACOBRAR.FECHAVCTO, dbo.CLIENTES.NUMCLIENTE, VENTAS.FECHAVENTA, dbo.VENTAS.NOMBRECLIENTE, dbo.VENTAS.CODCLIENTE, CASE MODALIDADPAGO WHEN 0 THEN 'CONTADO' WHEN 1 THEN 'CREDITO' WHEN 2 THEN 'BONIF' WHEN 3 THEN 'CAMBIO' ELSE 'OTROS' END AS TIPO, dbo.VENTAS.NUMVENTA, dbo.FACTURACOBRAR.IMPORTECUOTA, dbo.FACTURACOBRAR.SALDOCUOTA, DATEDIFF(D, dbo.FACTURACOBRAR.FECHAVCTO,CONVERT(DATETIME, @FCONSULTA, 103)) AS ATRASO, dbo.CLIENTES.CODTIPOCLIENTE, dbo.CLIENTES.CODCATEGORIACLIENTE FROM dbo.FACTURACOBRAR INNER JOIN dbo.VENTAS ON dbo.FACTURACOBRAR.CODVENTA = dbo.VENTAS.CODVENTA INNER JOIN dbo.CLIENTES ON dbo.VENTAS.CODCLIENTE = dbo.CLIENTES.CODCLIENTE WHERE (dbo.VENTAS.ESTADO = 1)) AS CLIENTESATRASADOSAUX" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="FillByCliente" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetDataByCliente" GeneratorSourceName="FillByCliente" GetMethodModifier="Public" GetMethodName="GetDataByCliente" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetDataByCliente" UserSourceName="FillByCliente">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, CODCLIENTE, NOMBRECLIENTE, TIPO, NUMVENTA, IMPORTECUOTA, SALDOCUOTA, 
                         ATRASO,FECHAVENTA
FROM            (SELECT        NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, NOMBRECLIENTE, TIPO, NUMVENTA, IMPORTECUOTA, SALDOCUOTA, 
                                                    CASE SALDOCUOTA WHEN 0 THEN 0 ELSE ATRASO END AS ATRASO, CODTIPOCLIENTE, CODCATEGORIACLIENTE, CODCLIENTE,FECHAVENTA
                          FROM            (SELECT        dbo.FACTURACOBRAR.CODNUMEROCUOTA AS NUMEROCUOTA, dbo.FACTURACOBRAR.FECHAVCTO, dbo.CLIENTES.NUMCLIENTE, VENTAS.FECHAVENTA,
                                                                              dbo.VENTAS.NOMBRECLIENTE, dbo.VENTAS.CODCLIENTE, 
                                                                              CASE MODALIDADPAGO WHEN 0 THEN 'CONTADO' WHEN 1 THEN 'CREDITO' WHEN 2 THEN 'BONIF' WHEN 3 THEN 'CAMBIO' ELSE 'OTROS'
                                                                               END AS TIPO, dbo.VENTAS.NUMVENTA, dbo.FACTURACOBRAR.IMPORTECUOTA, dbo.FACTURACOBRAR.SALDOCUOTA, DATEDIFF(D, 
                                                                              dbo.FACTURACOBRAR.FECHAVCTO,CONVERT(DATETIME, @FCONSULTA, 103)) AS ATRASO, dbo.CLIENTES.CODTIPOCLIENTE, 
                                                                              dbo.CLIENTES.CODCATEGORIACLIENTE
                                                    FROM            dbo.FACTURACOBRAR INNER JOIN
                                                                              dbo.VENTAS ON dbo.FACTURACOBRAR.CODVENTA = dbo.VENTAS.CODVENTA INNER JOIN
                                                                              dbo.CLIENTES ON dbo.VENTAS.CODCLIENTE = dbo.CLIENTES.CODCLIENTE
                                                    WHERE        (dbo.VENTAS.ESTADO = 1)) AS CLIENTESATRASADOSAUX) AS CLIENTESATRASADOS
WHERE        (ATRASO &gt; 0) AND (CODCLIENTE = @Codcliente) AND (CODTIPOCLIENTE IN
                             (SELECT        item
                               FROM            dbo.fnPartir(@LISTAP, ',') AS fnPartir_1)) AND (CODCATEGORIACLIENTE IN
                             (SELECT        item
                               FROM            dbo.fnPartir(@CATCLI, ',') AS fnPartir_1)) AND  (FECHAVCTO &lt;= CONVERT(DATETIME, @VTOAL, 103))
ORDER BY ATRASO DESC,NOMBRECLIENTE     </CommandText>
