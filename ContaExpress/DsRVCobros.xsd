                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT COMPROBANTES2.TOTAL, COMPROBANTES2.SALDO, COMPROBANTES2.NROCOMPROBANTE, COMPROBANTES2.FECHA, COMPROBANTES2.TIPO, COMPROBANTES2.NUMCLIENTE, 
                         COMPROBANTES2.NOMBRE, COMPROBANTES2.CODCLIENTE, COMPROBANTES2.DIRECCION, COMPROBANTES2.DESCIUDAD, COMPROBANTES2.NOMBREFANTASIA, COMPROBANTES2.TELEFONO, 
                         VENTAS_1.TOTALEXENTA, VENTAS_1.TOTALGRAVADA, VENTAS_1.TOTALIVA, VENTAS_1.TOTAL5, VENTAS_1.TOTAL10, VENTAS_1.TOTALGRAVADO5, VENTAS_1.TOTALGRAVADO10
FROM            (SELECT        TOTAL, CASE IDTRANSACCION WHEN 2 THEN SALDODEV ELSE SALDOVTA END AS SALDO, NROCOMPROBANTE, FECHA, 
                                                    CASE TIPOCOMP WHEN '0' THEN 'VTA_CONT' WHEN '1' THEN 'VTA_CRED' WHEN '2' THEN 'VTA_BONIF' WHEN '3' THEN 'VTA_CAMBIO' WHEN '4' THEN 'OTROS' WHEN 'Por Cambio' THEN 'NC_CAMBIO'
                                                     WHEN 'Bonificación' THEN 'NC_BONIF' ELSE 'NC_DEVOL' END AS TIPO, NUMCLIENTE, NOMBRE, CODCLIENTE, DIRECCION, DESCIUDAD, ESTADO, CODCATEGORIACLIENTE, CODTIPOCLIENTE, 
                                                    NOMBREFANTASIA, TELEFONO
                          FROM            (SELECT        TOP (100) PERCENT CCC.IDTRANSACCION, CASE IDTRANSACCION WHEN 2 THEN DEVOLUCION.ESTADO ELSE VENTAS.ESTADO END AS ESTADO, 
                                                                              dbo.CLIENTES.CODCATEGORIACLIENTE, dbo.CLIENTES.CODTIPOCLIENTE, 
                                                                              CASE WHEN idtransaccion = 2 THEN - dbo.DEVOLUCION.TOTALDEVOLUCION ELSE dbo.VENTAS.TOTALVENTA END AS TOTAL, CASE IDTRANSACCION WHEN 2 THEN -
                                                                                  (SELECT        dbo.DEVOLUCION.TOTALDEVOLUCION - ISNULL(SUM(DEBE), 0) AS Expr1
                                                                                    FROM            dbo.CLIENTESCUENTACORRIENTE AS CLCTACTE3
                                                                                    WHERE        (CODDEVOLUCION = CCC.CODDEVOLUCION) AND (FECHACLIENTEMOV &gt;= @fdesde) AND (FECHACLIENTEMOV &lt;= @fhasta)) ELSE 0 END AS SALDODEV, 
                                                                              CASE IDTRANSACCION WHEN 1 THEN
                                                                                  (SELECT        dbo.VENTAS.TOTALVENTA - ISNULL(SUM(HABER), 0) AS Expr1
                                                                                    FROM            dbo.CLIENTESCUENTACORRIENTE AS CLCTACTE4
                                                                                    WHERE        (CODVENTA = CCC.CODVENTA) AND (FECHACLIENTEMOV &gt;= @fdesde) AND (FECHACLIENTEMOV &lt;= @fhasta)) ELSE 0 END AS SALDOVTA, 
                                                                              CASE WHEN idtransaccion = 2 THEN dbo.DEVOLUCION.numDEVOLUCION ELSE dbo.VENTAS.NUMVENTA END AS NROCOMPROBANTE, CCC.FECHACLIENTEMOV AS FECHA, 
                                                                              CASE WHEN idtransaccion = 2 THEN DEVOLUCION.TIPODEVOLUCION ELSE CONVERT(VARCHAR, VENTAS.MODALIDADPAGO) END AS TIPOCOMP, dbo.CLIENTES.NUMCLIENTE, 
                                                                              dbo.CLIENTES.NOMBRE, dbo.CLIENTES.CODCLIENTE, dbo.CLIENTES.NOMBREFANTASIA, dbo.CLIENTES.TELEFONO, dbo.CLIENTES.DIRECCION, dbo.CIUDAD.DESCIUDAD, 
                                                                              CASE WHEN idtransaccion = 2 THEN dbo.DEVOLUCION.CODDEVOLUCION ELSE dbo.VENTAS.CODVENTA END AS CODCOMP
                                                    FROM            dbo.CLIENTESCUENTACORRIENTE AS CCC INNER JOIN
                                                                              dbo.CLIENTES ON CCC.CODCLIENTE = dbo.CLIENTES.CODCLIENTE LEFT OUTER JOIN
                                                                              dbo.CIUDAD ON dbo.CLIENTES.CODCIUDAD = dbo.CIUDAD.CODCIUDAD LEFT OUTER JOIN
                                                                              dbo.DEVOLUCION ON CCC.CODDEVOLUCION = dbo.DEVOLUCION.CODDEVOLUCION LEFT OUTER JOIN
                                                                              dbo.VENTAS ON CCC.CODVENTA = dbo.VENTAS.CODVENTA
                                                    WHERE        (CCC.IDTRANSACCION = 1) OR
                                                                              (CCC.IDTRANSACCION = 2)) AS COMPROBANTES) AS COMPROBANTES2 LEFT OUTER JOIN
                         dbo.VENTAS AS VENTAS_1 ON COMPROBANTES2.NROCOMPROBANTE = VENTAS_1.NUMVENTA
WHERE        (COMPROBANTES2.NUMCLIENTE BETWEEN @Numinicio AND @Numfin) AND (COMPROBANTES2.FECHA &gt;= @fdesde) AND (COMPROBANTES2.FECHA &lt;= @fhasta) AND (COMPROBANTES2.SALDO &lt;&gt; 0) AND 
                         (COMPROBANTES2.ESTADO = 1)
ORDER BY COMPROBANTES2.NOMBRE, COMPROBANTES2.FECHA</CommandText>
