                                                                                                        CLIENTES.CODTIPOCLIENTE, CLIENTES.CODCATEGORIACLIENTE
                                                                              FROM            FACTURACOBRAR INNER JOIN
                                                                                                        VENTAS ON FACTURACOBRAR.CODVENTA = VENTAS.CODVENTA INNER JOIN
                                                                                                        CLIENTES ON VENTAS.CODCLIENTE = CLIENTES.CODCLIENTE
                                                                              WHERE        (VENTAS.ESTADO = 1)) AS CLIENTESATRASADOSAUX) AS CLIENTESATRASADOS
                          WHERE        (SALDOCUOTA &gt; 0) AND (FECHAVENTA &lt;= CONVERT(DATETIME, @FECHAAL, 103))
                          UNION
                          SELECT        TOP (100) PERCENT NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, CODCLIENTE, NOMBRE, TIPODEVOLUCION, NUMDEVOLUCION, TOTALDEVOLUCION, SALDO, ATRASO,COTIZACION, FECHADEVOLUCION, 
                                                   CODTIPOCLIENTE, CODCATEGORIACLIENTE, 0 AS TIPOCOMP
                          FROM            (SELECT        NUMEROCUOTA, FECHAVCTO, NUMCLIENTE, NOMBRE, TIPODEVOLUCION, NUMDEVOLUCION, TOTALDEVOLUCION, SALDO, ATRASO, COTIZACION, CODTIPOCLIENTE, CODCATEGORIACLIENTE, 
                                                                              CODCLIENTE, FECHADEVOLUCION
                                                    FROM            (SELECT        1 AS NUMEROCUOTA, DEVOLUCION.FECHADEVOLUCION AS FECHAVCTO, CLIENTES_1.NUMCLIENTE, DEVOLUCION.FECHADEVOLUCION, CLIENTES_1.NOMBRE, 
                                                                                                        DEVOLUCION.CODCLIENTE, 
