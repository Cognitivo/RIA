                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        CONVERT(DATETIME, CAST(dbo.PRODENTRADASALIDA.FECHA AS VARCHAR(11)), 103) AS FECHA, SUM(dbo.PRODENTRADASALIDADET.CANTIDAD) 
                         AS CANTIDAD, SUM(dbo.PRODENTRADASALIDADET.COSTO) AS COSTO, dbo.PRODUCTOS.DESPRODUCTO, dbo.PRODENTRADASALIDADET.TIPO
FROM            dbo.PRODCONSUMOINTERNODET INNER JOIN
                         dbo.PRODCONSUMOINTERNO ON dbo.PRODCONSUMOINTERNODET.CODCONSUMO = dbo.PRODCONSUMOINTERNO.CODCONSUMO CROSS JOIN
                         dbo.PRODENTRADASALIDADET INNER JOIN
                         dbo.PRODENTRADASALIDA ON dbo.PRODENTRADASALIDADET.CODPRODUCCION = dbo.PRODENTRADASALIDA.CODPRODUCCION CROSS JOIN
                         dbo.PRODUCTOS
WHERE        (dbo.PRODENTRADASALIDA.FECHA &gt;= CONVERT(datetime, @FECHADESDE, 103)) AND (dbo.PRODENTRADASALIDA.FECHA &lt;= CONVERT(datetime, 
                         @FECHAHASTA, 103)) AND (dbo.PRODENTRADASALIDADET.TIPO = @MOV)
GROUP BY CONVERT(DATETIME, CAST(dbo.PRODENTRADASALIDA.FECHA AS VARCHAR(11)), 103), dbo.PRODUCTOS.DESPRODUCTO, 
                         dbo.PRODENTRADASALIDADET.TIPO</CommandText>
