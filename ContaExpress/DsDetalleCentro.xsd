                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT SUCURSAL.DESSUCURSAL, COMPRAS.FECHACOMPRA, COMPRAS.NUMCOMPRA, CENTROCOSTO.DESCENTRO, 
                         COMPRASDETALLE.DESPRODUCTO, COMPRASDETALLE.CANTIDADCOMPRA, COMPRASDETALLE.COSTOUNITARIO, 
                         COMPRASDETALLE.COSTOUNITARIO * COMPRASDETALLE.CANTIDADCOMPRA AS TOTALGENERAL, 
                         CASE IVA WHEN 10 THEN (dbo.COMPRASDETALLE.COSTOUNITARIO - dbo.COMPRASDETALLE.COSTOUNITARIO / 11) 
                         * dbo.COMPRASDETALLE.CANTIDADCOMPRA WHEN 5 THEN (dbo.COMPRASDETALLE.COSTOUNITARIO - dbo.COMPRASDETALLE.COSTOUNITARIO / 21) 
                         * dbo.COMPRASDETALLE.CANTIDADCOMPRA ELSE dbo.COMPRASDETALLE.COSTOUNITARIO * dbo.COMPRASDETALLE.CANTIDADCOMPRA END AS TOTALSINIVA
FROM            PROVEEDOR INNER JOIN
                         COMPRAS ON PROVEEDOR.CODPROVEEDOR = COMPRAS.CODPROVEEDOR INNER JOIN
                         COMPRASDETALLE ON COMPRAS.CODCOMPRA = COMPRASDETALLE.CODCOMPRA INNER JOIN
                         SUCURSAL ON COMPRAS.CODSUCURSAL = SUCURSAL.CODSUCURSAL AND COMPRASDETALLE.CODSUCURSAL = SUCURSAL.CODSUCURSAL INNER JOIN
                         CENTROCOSTO ON COMPRASDETALLE.CODCENTRO = CENTROCOSTO.CODCENTRO
WHERE        (COMPRAS.ESTADO = 1) AND (COMPRAS.FECHACOMPRA &gt;= @desde) AND (COMPRAS.FECHACOMPRA &lt;= @hasta) AND (SUCURSAL.DESSUCURSAL LIKE @sucursal)
                          AND (CENTROCOSTO.DESCENTRO LIKE @descentro)
ORDER BY COMPRAS.FECHACOMPRA</CommandText>
