                      <Parameter AllowDbNull="true" AutogeneratedName="sucursal" ColumnName="DESSUCURSAL" DataSourceName="ENVASESPARAGUAYOS.dbo.SUCURSAL" DataTypeServer="varchar(50)" DbType="AnsiString" Direction="Input" ParameterName="@sucursal" Precision="0" ProviderType="VarChar" Scale="0" Size="50" SourceColumn="DESSUCURSAL" SourceColumnNullMapping="false" SourceVersion="Current" />
