                    <CommandText>SELECT        TOP (100) PERCENT DEVOLUCION.FECHADEVOLUCION, DEVOLUCION.NUMDEVOLUCION, CLIENTES.CODCLIENTE, CLIENTES.NUMCLIENTE, CLIENTES.NOMBRE, DEVOLUCION.COTIZACION1, 
