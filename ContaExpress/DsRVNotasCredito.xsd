                         ZONA ON CLIENTES.CODZONA = ZONA.CODZONA LEFT OUTER JOIN
                         CIUDAD ON ZONA.CODCIUDAD = CIUDAD.CODCIUDAD LEFT OUTER JOIN
                         PAIS ON CIUDAD.CODPAIS = PAIS.CODPAIS</CommandText>
