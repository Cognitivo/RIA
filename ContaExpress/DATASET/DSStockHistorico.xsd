      <DataSource DefaultConnectionIndex="0" FunctionsComponentName="QueriesTableAdapter" Modifier="AutoLayout, AnsiClass, Class, Public" SchemaSerializationMode="IncludeSchema" xmlns="urn:schemas-microsoft-com:xml-msdatasource">
        <Connections>
          <Connection AppSettingsObjectName="MySettings" AppSettingsPropertyName="GESTIONConnectionString2" ConnectionStringObject="" IsAppSettingsProperty="true" Modifier="Assembly" Name="GESTIONConnectionString2 (MySettings)" ParameterPrefix="@" PropertyReference="ApplicationSettings.ContaExpress.My.MySettings.GlobalReference.Default.GESTIONConnectionString2" Provider="System.Data.SqlClient" />
        </Connections>
        <Tables>
          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="StockHistoricoTableAdapter" GeneratorDataComponentClassName="StockHistoricoTableAdapter" Name="StockHistorico" UserDataComponentName="StockHistoricoTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="GESTIONConnectionString2 (MySettings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (100) PERCENT dbo.MOVPRODUCTO.CODCODIGO, dbo.MOVPRODUCTO.CODDEPOSITO, SUM(dbo.MOVPRODUCTO.CANTIDAD) AS Totales, dbo.PRODUCTOS.DESPRODUCTO, dbo.CODIGOS.CODIGO, 
                         dbo.SUCURSAL.DESSUCURSAL
FROM            dbo.MOVPRODUCTO INNER JOIN
                         dbo.CODIGOS ON dbo.MOVPRODUCTO.CODCODIGO = dbo.CODIGOS.CODCODIGO INNER JOIN
                         dbo.PRODUCTOS ON dbo.CODIGOS.CODPRODUCTO = dbo.PRODUCTOS.CODPRODUCTO INNER JOIN
                         dbo.SUCURSAL ON dbo.MOVPRODUCTO.CODDEPOSITO = dbo.SUCURSAL.CODSUCURSAL
WHERE        (dbo.MOVPRODUCTO.CODCODIGO &gt; 0) AND (dbo.MOVPRODUCTO.CODDEPOSITO = @deposito) AND (dbo.MOVPRODUCTO.FECHAMOVIMIENTO &lt;= CONVERT(datetime, @fechamax, 121))
GROUP BY dbo.MOVPRODUCTO.CODCODIGO, dbo.MOVPRODUCTO.CODDEPOSITO, dbo.PRODUCTOS.DESPRODUCTO, dbo.CODIGOS.CODIGO, dbo.SUCURSAL.DESSUCURSAL
ORDER BY dbo.MOVPRODUCTO.CODCODIGO</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="true" AutogeneratedName="deposito" ColumnName="CODDEPOSITO" DataSourceName="raices.dbo.MOVPRODUCTO" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@deposito" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="CODDEPOSITO" SourceColumnNullMapping="false" SourceVersion="Current" />
                      <Parameter AllowDbNull="true" AutogeneratedName="fechamax" ColumnName="FECHAMOVIMIENTO" DataSourceName="raices.dbo.MOVPRODUCTO" DataTypeServer="datetime" DbType="DateTime" Direction="Input" ParameterName="@fechamax" Precision="0" ProviderType="DateTime" Scale="0" Size="8" SourceColumn="FECHAMOVIMIENTO" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
            <Mappings>
              <Mapping SourceColumn="CODCODIGO" DataSetColumn="CODCODIGO" />
              <Mapping SourceColumn="CODDEPOSITO" DataSetColumn="CODDEPOSITO" />
              <Mapping SourceColumn="Totales" DataSetColumn="Totales" />
              <Mapping SourceColumn="DESPRODUCTO" DataSetColumn="DESPRODUCTO" />
              <Mapping SourceColumn="CODIGO" DataSetColumn="CODIGO" />
              <Mapping SourceColumn="DESSUCURSAL" DataSetColumn="DESSUCURSAL" />
            </Mappings>
            <Sources />
          </TableAdapter>
          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="StockHistSimpleTableAdapter" GeneratorDataComponentClassName="StockHistSimpleTableAdapter" Name="StockHistSimple" UserDataComponentName="StockHistSimpleTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="GESTIONConnectionString2 (MySettings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT SUCURSAL.DESSUCURSAL,PRODUCTOS.DESPRODUCTO,SUMQTY.QTY_SUM,MAXVALOR.VALOR FROM
(SELECT MOVPRODUCTO.CODCODIGO, SUM(CANTIDAD) AS QTY_SUM,MOVPRODUCTO.CODDEPOSITO
FROM MOVPRODUCTO 
WHERE FECHAMOVIMIENTO &lt; @FECHA
GROUP BY MOVPRODUCTO.CODCODIGO,MOVPRODUCTO.CODDEPOSITO)SUMQTY
JOIN
(SELECT MP.VALOR,MP.CODCODIGO,MP.CODDEPOSITO FROM 
MOVPRODUCTO MP JOIN
(SELECT 
	MAX(FECHAMOVIMIENTO) MAXFECHA,CODCODIGO 
FROM MOVPRODUCTO 
WHERE COSTEABLE = 1 AND FECHAMOVIMIENTO &lt; @FECHA
GROUP BY MOVPRODUCTO.CODCODIGO,MOVPRODUCTO.CODDEPOSITO)MAX_FECHA
ON MAX_FECHA.CODCODIGO = MP.CODCODIGO AND
MAX_FECHA.MAXFECHA = MP.FECHAMOVIMIENTO)MAXVALOR 
ON SUMQTY.CODCODIGO = MAXVALOR.CODCODIGO 
JOIN CODIGOS 
ON CODIGOS.CODCODIGO = SUMQTY.CODCODIGO
LEFT OUTER JOIN PRODUCTOS
ON PRODUCTOS.CODPRODUCTO = CODIGOS.CODPRODUCTO
LEFT OUTER JOIN SUCURSAL
ON SUMQTY.CODDEPOSITO = SUCURSAL.CODSUCURSAL</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="true" AutogeneratedName="FECHA" ColumnName="" DataSourceName="" DataTypeServer="datetime" DbType="DateTime" Direction="Input" ParameterName="@FECHA" Precision="0" Scale="0" Size="8" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
            <Mappings>
              <Mapping SourceColumn="DESSUCURSAL" DataSetColumn="DESSUCURSAL" />
              <Mapping SourceColumn="DESPRODUCTO" DataSetColumn="DESPRODUCTO" />
              <Mapping SourceColumn="QTY_SUM" DataSetColumn="QTY_SUM" />
              <Mapping SourceColumn="VALOR" DataSetColumn="VALOR" />
            </Mappings>
            <Sources />
          </TableAdapter>
        </Tables>
        <Sources />
      </DataSource>
    </xs:appinfo>
  </xs:annotation>
